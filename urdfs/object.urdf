<?xml version="1.0"?>
<robot name="box">
  <!-- Colors -->
  <material name="black">
      <color rgba="0 0 0 1"/>
  </material>

  <link name="link_name">         <!--一つの部分-->
    <visual>                      <!--その部分の表示-->
      <geometry>
        <!--box size="0.5 0.5 0.1"/-->
        <cylinder length="0.2" radius="0.3"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>                   <!--その部分の衝突データ-->
      <geometry>
        <!--box size="0.5 0.5 0.1"/-->
        <cylinder length="0.2" radius="0.3"/>
      </geometry>
    </collision>
    <inertial>                     <!--その部分の慣性データ-->
            <mass value="0.15"/>
            <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0.0" izz="0.2"/>
        </inertial>
  </link>

</robot>
